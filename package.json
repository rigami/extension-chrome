{
    "name": "rigami",
    "version": "0.19.15",
    "description": "Rigami",
    "main": "index.js",
    "scripts": {
        "build": "NODE_ENV=production webpack",
        "build-wallpapers": "BUILD=wallpapers yarn build",
        "build:clear": "yarn rimraf ./release/* && mkdirp ./release",
        "build:zip": "zip -r release/rigami-$npm_package_version.zip build",
        "build:release": "COLLECT_LOGS=true yarn build && yarn build:clear && yarn build:zip && yarn release",
        "build-wallpapers:release": "COLLECT_LOGS=true yarn build-wallpapers && yarn build:clear && yarn build:zip && yarn release",
        "build:analyze": "ANALYZE=true yarn build",
        "build-wallpapers:analyze": "ANALYZE=true yarn build-wallpapers",
        "dev": "NODE_ENV=development webpack-cli serve",
        "dev:prod-env": "NODE_ENV=development PRODUCTION_ENV=true webpack-cli serve",
        "dev-wallpapers": "NODE_ENV=development BUILD=wallpapers webpack-cli serve",
        "dev-wallpapers:prod-env": "NODE_ENV=development BUILD=wallpapers PRODUCTION_ENV=true webpack-cli serve",
        "lint": "eslint --fix --ext .jsx,.js,.json --ignore-path .gitignore .",
        "release": "git tag -a v$npm_package_version && git push --tags",
        "bump": "yarn bump:patch",
        "bump:patch": "yarn version --patch --no-commit --no-git-tag-version && git add ./package.json",
        "bump:minor": "yarn version --minor --no-commit --no-git-tag-version && git add ./package.json",
        "bump:major": "yarn version --major --no-commit --no-git-tag-version && git add ./package.json",
        "prepare": "husky install"
    },
    "repository": "https://github.com/rigami/extension-chrome",
    "author": "Danilkinkin <Danilkinkin@gmail.com>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/rigami/readme/issues"
    },
    "homepage": "https://rigami.io",
    "dependencies": {
        "@material-ui/core": "^4.11.4",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.58",
        "@material-ui/styles": "^4.11.4",
        "@sentry/react": "^6.4.1",
        "@sentry/tracing": "^6.4.1",
        "clsx": "^1.1.1",
        "copy-to-clipboard": "^3.3.1",
        "fast-xml-parser": "^3.19.0",
        "i18next": "^20.3.0",
        "i18next-http-backend": "^1.2.4",
        "idb": "^6.1.0",
        "jszip": "^3.6.0",
        "lodash": "^4.17.21",
        "mobx": "^6.3.2",
        "mobx-react-lite": "^3.2.0",
        "notistack": "^1.0.9",
        "object-hash": "^2.1.1",
        "prop-types": "^15.7.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-i18next": "^11.8.15",
        "react-resize-detector": "^6.7.2",
        "react-scrollbars-custom": "^4.0.25"
    },
    "devDependencies": {
        "@babel/core": "^7.14.3",
        "@babel/eslint-parser": "^7.14.3",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-proposal-decorators": "^7.14.2",
        "@babel/plugin-syntax-throw-expressions": "^7.12.13",
        "@babel/plugin-transform-react-jsx": "^7.14.3",
        "@babel/preset-env": "^7.14.2",
        "@babel/preset-react": "^7.12.13",
        "@babel/traverse": "^7.14.2",
        "@babel/types": "^7.14.2",
        "@sentry/webpack-plugin": "^1.15.1",
        "babel-loader": "^8.2.2",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-plugin-transform-decorators": "^6.24.1",
        "babel-plugin-transform-imports": "^2.0.0",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^8.0.0",
        "css-loader": "^5.2.5",
        "eslint": "^7.27.0",
        "eslint-config-rigami": "rigami/eslint-config-rigami",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-webpack": "^0.13.1",
        "file-loader": "^6.0.0",
        "generate-json-from-js-webpack-plugin": "^0.1.1",
        "git-branch-is": "^4.0.0",
        "html-webpack-plugin": "^5.3.1",
        "husky": "^6.0.0",
        "json-loader": "^0.5.7",
        "null-loader": "^4.0.1",
        "pnp-webpack-plugin": "^1.6.4",
        "react-svg-loader": "^3.0.3",
        "style-loader": "^2.0.0",
        "webpack": "^5.37.1",
        "webpack-bundle-analyzer": "^4.4.2",
        "webpack-cli": "^4.7.0",
        "webpack-dev-server": "^3.11.2",
        "workbox-core": "^6.1.5",
        "workbox-precaching": "^6.1.5",
        "workbox-routing": "^6.1.5",
        "workbox-webpack-plugin": "^6.1.5"
    },
    "sideEffects": false
}
