{
    "name": "rigami",
    "version": "0.1.116",
    "description": "Rigami",
    "main": "index.js",
    "scripts": {
        "build": "NODE_ENV=production webpack",
        "build:zip": "yarn build && yarn rimraf ./release/* && mkdirp ./release && zip -r release/rigami-$npm_package_version.zip build",
        "build:release": "yarn build:zip && yarn release",
        "build:analyze": "ANALYZE=true yarn build",
        "dev": "NODE_ENV=development webpack-cli serve",
        "lint": "eslint --fix --ext .jsx,.js,.json --ignore-path .gitignore .",
        "release": "git tag -a v$npm_package_version && git push --tags",
        "bump": "yarn bump:patch",
        "bump:patch": "yarn version --patch --no-commit --no-git-tag-version && git add ./package.json",
        "bump:minor": "yarn version --minor --no-commit --no-git-tag-version && git add ./package.json",
        "bump:major": "yarn version --major --no-commit --no-git-tag-version && git add ./package.json"
    },
    "husky": {
        "hooks": {
            "pre-commit": "git-branch-is --not master && yarn bump:patch || echo 'skip set version'"
        }
    },
    "repository": "https://github.com/rigami/extension-chrome",
    "author": "Danilkinkin <Danilkinkin@gmail.com>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/rigami/readme/issues"
    },
    "homepage": "https://rigami.io",
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "@material-ui/styles": "^4.10.0",
        "clsx": "^1.1.1",
        "i18next": "^19.6.3",
        "i18next-browser-languagedetector": "^6.0.1",
        "i18next-http-backend": "^1.0.18",
        "idb": "^5.0.4",
        "lodash": "^4.17.20",
        "mobx": "^6.0.1",
        "mobx-react-lite": "^3.0.1",
        "notistack": "^1.0.1",
        "prop-types": "^15.7.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-i18next": "^11.7.0",
        "react-indiana-drag-scroll": "^1.6.1",
        "react-resize-detector": "^5.0.7",
        "react-scrollbars-custom": "^4.0.25",
        "smooth-scrollbar": "^8.5.2"
    },
    "devDependencies": {
        "@babel/core": "^7.11.1",
        "@babel/eslint-parser": "^7.11.5",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-proposal-decorators": "^7.10.5",
        "@babel/plugin-syntax-throw-expressions": "^7.10.4",
        "@babel/plugin-transform-react-jsx": "^7.10.4",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-react": "^7.10.4",
        "@babel/traverse": "^7.11.0",
        "@babel/types": "^7.11.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "babel-plugin-transform-decorators": "^6.24.1",
        "babel-plugin-transform-imports": "^2.0.0",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^6.2.1",
        "css-loader": "^5.0.0",
        "eslint": "^7.7.0",
        "eslint-config-rigami": "rigami-org/eslint-config-rigami",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-webpack": "^0.13.0",
        "file-loader": "^6.0.0",
        "generate-json-from-js-webpack-plugin": "^0.1.1",
        "git-branch-is": "^4.0.0",
        "html-webpack-plugin": "^4.5.0",
        "husky": "^4.3.0",
        "json-loader": "^0.5.7",
        "pnp-webpack-plugin": "^1.6.4",
        "react-svg-loader": "^3.0.3",
        "style-loader": "^2.0.0",
        "webpack": "^5.3.2",
        "webpack-bundle-analyzer": "^3.8.0",
        "webpack-cli": "^4.1.0",
        "webpack-dev-server": "^3.11.0"
    }
}
