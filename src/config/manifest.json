{
  "manifest_version": 2,

  "name": "ClockTab - Новая вкладка",
  "short_name": "ClockTab",
  "version": "2.0.00",
  "homepage_url": "http://danilkinkin.com/projects/clockTab/",
  "minimum_chrome_version": "48",
  "offline_enabled": true,
  "icons":{
    "16":"resource/16x16.png",
    "32":"resource/32x32.png",
    "64":"resource/64x64.png",
    "128":"resource/128x128.png"
  },

  "description":"Простая и красивая новая вкладка для chrome",
  "permissions":[
    "activeTab",
    "storage",
    "unlimitedStorage",
    "*://*/*",
    "http://danilkinkin.com/",
    "notifications"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}